        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".LoginAddActivity" />
        <activity android:name=".MainSellerActivity" />
        <activity android:name=".MainBuyerActivity" />
        <activity android:name=".ItemAddActivity" />
